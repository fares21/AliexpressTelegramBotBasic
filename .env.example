# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file to git!

# Telegram Bot Token (get from @BotFather)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# AliExpress API Credentials (get from AliExpress Open Platform)
ALIEXPRESS_API_PUBLIC=your_aliexpress_api_public_key
ALIEXPRESS_API_SECRET=your_aliexpress_api_secret_key

# Optional: Webhook URL for production (leave empty for development polling mode)
# WEBHOOK_URL=https://your-domain.com/webhook

# Optional: Your Telegram Channel ID
CHANNEL_ID=@YourChannelName

